include index

#banner.container
      .row
        .col-lg-10
          .page-header
            h1 Projects
      .row
        each project in projects              
            .col-lg-6.col-md-7.col-sm-10         
              .panel.panel-primary
                - var url = project.id;
                      .panel-heading
                        h3.panel-title
                          a(href="projects/" + url) #{project.title}
                      .panel-body
                        p #{project.description}
                        a(href="#").btn.btn-primary Read More
                      
                          
