extends index

block main
      #banner.container
          .row
            .col-lg-10
              .page-header
                h1 Projects
          .row
              .panel.panel-primary
                each project in projects
                  - var url = project.id;
                        .panel-heading
                          h3.panel-title
                            a(href="projects/" + url) #{project.title}
                        .panel-body
                          | #{project.description}
    
