extends index

block main
    #banner
            .row
            if (success_msg)
              div.alert.alert-success #{success_msg}
            else if (error_msg)
              div.alert.alert-danger #{error_msg}
                .col-lg-5
                    .page-header
                        h1
                            | #{firstName} #{lastName}
            .row
                .col-lg-3
                    img(src="#")
                .col-lg-6
                    p
                        | Username #{username}
                    p
                        | ID #{_id}
            .row
